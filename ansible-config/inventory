[chat_servers]
chat-server ansible_host=3.212.184.245 ansible_user=ec2-user ansible_ssh_private_key_file=~/.ssh/tom.pem

[jenkins_servers]
jenkins-server ansible_host=52.1.160.108 ansible_user=ec2-user ansible_ssh_private_key_file=~/.ssh/tom.pem

[chat_servers:vars]
ansible_ssh_common_args='-o StrictHostKeyChecking=no'

[jenkins_servers:vars]
ansible_ssh_common_args='-o StrictHostKeyChecking=no'